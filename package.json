{
  "name": "simple-stash",
  "version": "0.0.6",
  "description": "Simple local caching and lookup",
  "main": "lib/simple-stash.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "nyc mocha"
  },
  "keywords": [],
  "author": "Cody Dalton <codydevbox@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codyjdalton/simple-stash"
  },
  "nyc": {
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "./node_modules"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "sourceMap": true,
    "instrument": true
  },
  "devDependencies": {
    "chai": "*",
    "coveralls": "*",
    "mocha": "*",
    "nyc": "*"
  },
  "dependencies": {}
}
